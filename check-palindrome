def check_palindrome(astring):
    left_point, right_point = 0, len(astring) - 1
    while left_point < right_point:
        if astring[left_point] != astring[right_point]:
            return False
        left_point += 1
        right_point -= 1
    return True

def fib(num):
    if num <= 1:
        return 1
    return fib(num-1) + fib(num-2)

print(fib(30))